#include <iostream>
#include <stdlib.h>
#include <ctime>
using namespace std;

int detalleCliente[10000][3];
void llamarDC(){
    for (int i=0; i<10000; i++){
        detalleCliente[i][0]=rand()%2;
        detalleCliente [i][1]=rand()%83+1943;
        detalleCliente [i][2]=rand()%195;
    }
}
void imprimirDC(){
    cout<<"SEXO:     EDAD:     PAIS: "<<endl;
    for (int i=0; i<10000; i++){
        cout<<"  "<<detalleCliente[i][0]<<"       "<<detalleCliente[i][1]<<"       "<<detalleCliente[i][2]<<endl;
    }
}

int main(){
    
    srand(time(0));
    llamarDC();
    imprimirDC();
    
    return 0;
}
